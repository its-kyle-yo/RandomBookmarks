{
  "name": "Random Bookmark",
  "version": "0.0.1",
  "author": "Kyle Thomas",
  "description": "Helps you clear out your old and stale bookmarks by giving you a random one to use!",
  "permissions": ["activeTab", "declarativeContent", "storage", "bookmarks"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "scripts": ["popup-bundle.js"],
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}